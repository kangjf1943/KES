	year = {2012},
	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1890/11-0561.1},
	keywords = {GIS, ecosystem function, air quality, ecophysiology, Italy, Rome, sanitary benefits, tropospheric ozone, urban forest},
	pages = {349--360},
	file = {Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\ZHQPLCXI\\11-0561.html:text/html;Submitted Version:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\H5A9YGU4\\Manes et al. - 2012 - Urban ecosystem services tree diversity and stabi.pdf:application/pdf},
