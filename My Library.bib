	keywords = {Urban ecology, Spatial patterns, Carbon, Landscape metrics, Recreation, Urban-rural gradient},
	pages = {1068--1080},
	file = {ScienceDirect Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\5F6TIJX8\\S1470160X17307744.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\RF53K2IV\\Dobbs et al. - 2018 - Exploring temporal dynamics of urban ecosystem ser.pdf:application/pdf},
