	keywords = {Ecosystem services, Dynamic assessment, Equivalent factor, Sustainability, Valuation method},
	pages = {146--154},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\A2LYGWAC\\Xie et al. - 2017 - Dynamic changes in the value of China’s ecosystem .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\M3A2PMLF\\S2212041616301188.html:text/html},
