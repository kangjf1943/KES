@article{capotorti_combining_2017,
	title = {Combining the {Conservation} of {Biodiversity} with the {Provision} of {Ecosystem} {Services} in {Urban} {Green} {Infrastructure} {Planning}: {Critical} {Features} {Arising} from a {Case} {Study} in the {Metropolitan} {Area} of {Rome}},
