	author = {Loreau, Michel},
	month = may,
	year = {1998},
	pmid = {9576935},
	note = {Publisher: National Academy of Sciences
Section: Biological Sciences},
	keywords = {plant biomass, productivity, resource competition, soil nutrients, spatial heterogeneity},
	pages = {5632--5636},
	file = {Full Text PDF:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\FUSVUS2I\\Loreau - 1998 - Biodiversity and ecosystem functioning A mechanis.pdf:application/pdf;Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\XATYNT48\\5632.html:text/html},
